<div id="contentpage">
    <div id="headerTitle">
        <div class="row">
            <img id="imgLeft" src="data:image/png;base64,"
                alt="masks" width="120" height="80">
            <div id="headerTitleText">Make || Break</div>
            <img id="imgRight" src="data:image/png;base64,"
                alt="masks" width="120" height="80">
        </div>
    </div>



    <!--Generator Row-->

    <div class="row">
        <div class="col-md-2"></div>
        <div class="col-md-2 genColumn">
            <div class="row genRow">
                <form>
                    <div class="form-group">
                        <label for="insultInputs" class="genText">Insult Generator</label>
                            <p id = "genInsult"></p>
                    </div>
                </form>
            </div>
            <div class="row genRow">
                <button id="insulButt" type="button" class="btn btn-primary pgButt contentButt">Generate</button>
                <button class="btn btn-primary pgButt saveButt contentButt" type="button">Save</button>
                <button class="btn btn-primary pgButt sendButt contentButt" type="button">Send</button>
            </div>
        </div>
        <div class="col-md-1"></div>
        <div class="col-md-1"></div>
        <div class="col-md-2 genColumn">
            <div class="row genRow">
                <form>
                    <div class="form-group">
                        <label for="complimentInputs" class="genText">Compliment Generator</label>

                        <p id="genComp"></p>
                    </div>
                </form>
            </div>
            <div class="row genRow">

                <button id="compButt" type="button" class="btn btn-primary pgButt contentButt">Generate</button>
                <button class="btn btn-primary pgButt saveButt contentButt" type="button">Save</button>
                <button id = "compSendButt" class="btn btn-primary pgButt sendButt contentButt" type="button">Send</button>
            </div>
        </div>
        <div class="col-md-2"></div>
    </div>

    <!--Saved Row-->

    <div class="row"></div>

    <!--Insult/Compliment Row-->

    <div class="row cardrow">
        <div class="col-md-2"></div>
        <div class="card cards" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">Popular Insults</h5>
               
                <p class="card-text"></p>
            </div>
        </div>
        <div class="col-md-1 col-lg-2"></div>
        <div class="card cards" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">Popular Compliments</h5>
                
                <p class="card-text"></p>
            </div>
        </div>
        <div class="col-md-2"></div>
    </div>

    <!--Send Modal-->

    <div id="sendModal" class="modal hide" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="modal-title">Please Login</h1>
                </div>
                <div class="modal-body">
                    <form class="contButt">
                        <div class="form-group">
                            <label for="inputPhonenumber">Phone Number</label>
                            <input type="phonenumber" class="form-control" id="phonenumber" aria-describedby="emailHelp" placeholder="Enter Phone Number">
                            <small class="form-text text-muted">We'll never store the the Phone Number</small>
                        </div>
                        <div class="form-group">
                            <label for="inputInsComp">Message</label>
                            <p id = "messageBody">hello</p>
                        </div>
                        <button type="submit" id = "sendMessage" class="btn btn-primary pgButt sendButt">Send</button>
                    </form>
                </div>
            </div>
        </div>
    </div>

</div>

<script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
    crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<script src="/js/index.js"></script>

<script type="text/javascript">
    $(".sendButt").on("click", function (event) {
        event.preventDefault();
        $("#sendModal").modal("show");
    });
</script>